{
    "name": "aizhar777/laravel_mobizon",
    "description": "Mobizon Notifications channel for Laravel",
    "keywords": ["laravel", "notifications", "mobizon", "sms"],
    "homepage": "https://github.com/aizhar777/laravel_mobizon",
    "license": "MIT",
    "authors": [
        {
            "name": "Aizhar777",
            "role": "Developer",
            "email": "aizhar777@mail.ru",
            "homepage": "https://github.com/aizhar777"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/queue": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/notifications": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.5.*|5.6.*",
        "mobizon/mobizon-php": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Aizhar777\\Mobizon\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aizhar777\\Mobizon\\MobizonServiceProvider"
            ]
        }
    }
}
